<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lonelyvegan.facevalue" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Face Value</name>
    <description>
        Currency conversion using actual denominations.
    </description>
    <author email="matt@lonelyvegan.com" href="http://tofumatt.com">
        Matthew Riley MacPherson
    </author>
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
</widget>
